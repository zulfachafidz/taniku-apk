<Signup>:
    MDFloatLayout:
        md_bg_color: 1,1,1,1
    MDIconButton:
        icon: "arrow-left"
        pos_hint: {'center_x': 0.02,'center_y': 0.9}
        on_release:
            root.manager.current = "welcome"
    MDLabel:
        text: "S I G N  U P"
        font_name: "Bpoppins"
        font_size: 40
        halign: "center"
        pos_hint: {'center_x': 0.5,'center_y': 0.9}
        height: self.texture_size[1]
        padding_y: 30
    MDTextFieldRound:
        hint_text: "username"
        size_hint_x: None
        icon_left: "account"
        width: 270
        font_size: 20
        pos_hint: {'center_x': 0.5,'center_y': 0.75} 
        normal_color:[35/255,49/255,48/255,1]
        color_active: [1,1,1,1]
    MDTextFieldRound:
        hint_text: "Email"
        size_hint_x: None
        icon_left: "email"
        width: 270 
        font_size: 20
        pos_hint: {'center_x': 0.5,'center_y': 0.65}
        normal_color: [35/255,49/255,48/255,1]
        color_active: [1,1,1,1]
    MDTextFieldRound:
        hint_text: "password"
        size_hint_x: None
        icon_left: "key"
        width: 270 
        font_size: 20
        pos_hint: {'center_x': 0.5,'center_y': 0.55}
        normal_color: [35/255,49/255,48/255,1]
        color_active: [1,1,1,1]
    MDTextFieldRound:
        hint_text: "confirm password"
        size_hint_x: None
        icon_left: "key"
        width: 270 
        font_size: 20
        pos_hint: {'center_x': 0.5,'center_y': 0.45}
        normal_color: [35/255,49/255,48/255,1]
        color_active: [1,1,1,1]
    MDTextFieldRound:
        hint_text: "Mobile Number"
        size_hint_x: None
        icon_left: "phone"
        width: 270 
        font_size: 20
        pos_hint: {'center_x': 0.5,'center_y': 0.35}
        normal_color: [35/255,49/255,48/255,1]
        color_active: [1,1,1,1]
    MDFillRoundFlatButton:
        text: "SIGN UP"
        font_size: 15
        size_hint: .3, .08
        pos_hint: {'center_x': 0.5,'center_y': 0.25}
        on_release: root.manager.current = "login"

        